            "schema": {
              "format": "string"
            }
          }
        },
        "parameters": [
          {
            "name": "year",
            "in": "path",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1PersonRequest"
            }
          }
        ],
        "tags": [
          "PredictorService"
        ]
      }
    }
  },
